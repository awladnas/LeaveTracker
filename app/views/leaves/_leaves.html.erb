<table class="table table-bordered">
  <thead>
  <th>Start Date</th>
  <th>End Date</th>
  <th>Total Days</th>
  <th>Total Minutes</th>
  <th>Status</th>
  </thead>
  <% leaves.each do |leave| %>
      <tr>
        <td><%= leave.start_date %></td>
        <td><%= leave.end_date %></td>
        <td><%= leave.total_days %></td>
        <td><%= leave.total_minutes %></td>
        <td><%= leave.leave_status %> </td>
      </tr>
  <% end %>
</table>